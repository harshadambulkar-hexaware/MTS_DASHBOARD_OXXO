<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.hexaware.insight360DataPull.model">
	<class name="UserProjectMapping" table="user_project_mapping">
		<id name="map_id" column="mapId">
			<generator class="native"/>
		</id>
		<property name="user_id" column="userID" />
		<property name="project_id" column="projectID" />
		
	</class>	
</hibernate-mapping>